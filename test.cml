
external void printf(char* fmt);

export int main()
{
	int i = 0;
	int c;
	while (i < 10) {
		c = 0;
		c = (int)(c | i + 48);
		printf((char*)&c);
		printf(": ");
		if (i % 3 == 0)
			printf("bar\n");
		else if (i % 5 == 0)
			printf("baz\n");
		else
			printf("sad smile\n");
		i = i + 1;
	}

	return 228;
}