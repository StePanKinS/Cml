
namespace clib
{
    external int printf(char* fmt, void* ptr, void* ptr2);
}
import clib;

struct Point {
    int x,
    int y

    Point new(int n)
    {
        return Point { x: n, y: n };
    }

    void print(Point* self)
    {
        printf("Point(%d, %d)\n", (void*)(ulong)(long)self.x, (void*)(ulong)(long)self.y);
    }
}

export int main(int argc, char** argv)
{
    Point p = Point { x: 10, y: 5 };
    p.print();
    Point pp = Point.new(7);
    pp.print();
    return 0;
}
